package lift;

public class LiftThread extends LiftView implements Runnable {
	private PassengerThread[] passengers;
	
	public LiftThread() {
		super();
		
		passengers = new PassengerThread[20];
		for(int i=0; i<20; i++) {
			passengers[i] = new PassengerThread();
			passengers[i].start();
		}
	
	}

	@Override
	public void run() {
		// TODO Auto-generated method stub
		
	}

}
