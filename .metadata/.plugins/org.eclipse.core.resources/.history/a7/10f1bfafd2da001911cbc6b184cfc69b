import lift.LiftView;
import lift.LiftThread;
import lift.Passenger;
import lift.PassengerThread;

public class TwentyPeopleLiftin {

	public static void main(String[] args) {

		LiftThread view = new LiftThread();
		
		PassengerThread[] passengers = new PassengerThread[20];
		for(int i=0; i<20; i++) {
			passengers[i] = (PassengerThread) view.createPassenger();		
			passengers[i].start();
		}

	}

}
