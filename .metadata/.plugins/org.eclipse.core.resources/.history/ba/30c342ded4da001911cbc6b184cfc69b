
public class IndustryFourPoint0 {
	int here; // If here!=next, here (floor number) tells from which floor
	// the lift is moving and next to which floor it is moving.
	int next; // If here==next, the lift is standing still on the floor
	// given by here.
	int[] waitEntry; // Number of passengers waiting to enter the lift at the
	// various floors.
	int[] waitExit; // Number of passengers (inside the lift) waiting to leave
	// the lift at the various floors.
	int load; // Number of passengers currently in the lift.
	
	public IndustryFourPoint0(){
		
	}
	
}
